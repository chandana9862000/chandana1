@model IEnumerable<WebApplication4.Models.Tutorial>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<table class="table table-bordered table-hover table-light">
    <thead>
        <tr>
            <th>TutorialId</th>
            <th>TutorialName</th>
            <th>TutorialDesc</th>
            <th>Published</th>
            <th>TutorialFees</th>
            <th>Edit</th>
            <th>Delete</th>

        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (var item in Model)
            {
        <tr>
            <td>@item.TutorialId</td>
            <td>@item.TutorialName</td>
            <td>@item.TutorialDesc</td>
            <td>@item.Published</td>
            <td>@item.TutorialFees</td>
            <td><a asp-action="EditTutorial" asp-route-TutorialId="@item.TutorialId">Edit</a></td>
            <td><a asp-action="DeleteTutorial" asp-route-TutorialId="@item.TutorialId">Delete</a></td>
            

        </tr>
            }

    </tbody>

</table>
