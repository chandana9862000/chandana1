@model WebApplication4.Models.Tutorial
<hr />
<a class="btn btn-primary" asp-action="Index">Show Tutorial Details</a>
<hr />
<h2>Edit tutorial</h2>
<form method="post">
    <div>
        <h1>Tutorial Entry</h1>
        <hr />
        <table>
            <tr>
                <td>Tutorial Id</td>
                <td>
                    @*@Html.TextBoxFor(obj=>obj.TutorialId)*@
                    <input type="text"  asp-for="TutorialId" />
                </td>

            </tr>
            <tr>
                <td>Tutorial Name</td>
                <td>
                    @*@Html.TextBoxFor(obj =>obj.TutorialName)*@
                    <input type="text"  asp-for="TutorialName" />
                </td>
            </tr>
            <tr>
                <td>Tutorial Description</td>
                <td>
                    @*@Html.TextBoxFor(obj =>obj.TutorialDesc)*@
                    <input type="text"  asp-for="TutorialDesc" />
                </td>
            </tr>
            <tr>
                <td>Published</td>
                <td>
                    @*@Html.TextBoxFor(obj=>obj.Published)*@
                    <input type="text"  asp-for="Published" />
                </td>
            </tr>
            <tr>
                <td>Tutorial fees</td>
                <td>
                    @*@Html.TextBoxFor(obj=>obj.TutorialFees)*@
                    <input type="text"  asp-for="TutorialFees" />
                </td>
            </tr>
            <tr>
                <td></td>
                <td><input id="Submit1" type="submit" value="update" /></td>
            </tr>
        </table>
        <hr />
        <div>
            @ViewBag.message
        </div>
    </div>
</form>
